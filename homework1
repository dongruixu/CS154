//实验一第4题
#include <iostream>

using namespace std;

int main()   //输出特定图形
{
    cout << "  R\n";
    cout << " RSR\n";
    cout << "RSSSR\n";
    cout << " RSR\n";
    cout << "  R\n";

    return 0;
}


//实验一第5题
#include <iostream>

using namespace std;

int main()    //输出特定图形
{
    cout << "1" << endl;
    cout << "2\t4" << endl;
    cout << "3\t6\t9" << endl;
    cout << "4\t8\t12" << endl;
    cout << "5\t10\t15\t20" << endl;

    return 0;
}
